{
  "name": "livrolog",
  "author": "Arnon Rodrigues <arnonrdp@gmail.com> (https://arnon.dev)",
  "description": "A place for you to organize everything you've read.",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "yarn lint:oxlint && yarn lint:eslint",
    "format": "prettier --write .",
    "quality": "yarn type-check && yarn lint && yarn build",
    "format-check": "prettier --check src/",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:prod": "playwright test --config=playwright.production.config.ts"
  },
  "dependencies": {
    "@google-cloud/local-auth": "^3.0.1",
    "@quasar/extras": "^1.17.0",
    "axios": "^1.13.2",
    "echarts": "^6.0.0",
    "google-auth-library": "^10.5.0",
    "html2canvas": "^1.4.1",
    "jwt-decode": "^4.0.0",
    "pinia": "^3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "quasar": "^2.18.6",
    "secure-ls": "^2.0.0",
    "vite-plugin-vue-devtools": "^8.0.5",
    "vue": "^3.5.26",
    "vue-echarts": "^8.0.1",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@pinia/testing": "^0.1.7",
    "@playwright/test": "^1.52.0",
    "@quasar/vite-plugin": "^1.10.0",
    "@tsconfig/node22": "^22.0.5",
    "@types/node": "^24.3.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.0",
    "eslint": "^9.39.2",
    "eslint-plugin-oxlint": "^1.36.0",
    "eslint-plugin-vue": "^10.6.2",
    "happy-dom": "^20.0.11",
    "jiti": "^2.6.1",
    "jsdom": "^27.4.0",
    "npm-run-all2": "^8.0.4",
    "oxlint": "^1.36.0",
    "prettier": "^3.7.4",
    "sass-embedded": "^1.80.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^1.2.0",
    "vite-plugin-static-copy": "^3.1.4",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.2.1"
  }
}
