{
  "[html]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[php]": {
    "editor.defaultFormatter": "junstyle.php-cs-fixer",
    "editor.formatOnSave": false
  },
  "[sass]": {
    "editor.defaultFormatter": "syler.sass-indented"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": null
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "always",
    "source.organizeImports": "always"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "eslint.format.enable": true,
  "eslint.run": "onSave",
  "eslint.useFlatConfig": true,
  "eslint.validate": ["javascript", "typescript", "vue"],
  "files.associations": {
    "*.yml": "plaintext"
  },
  "github-actions.workflows.pinned.refresh.enabled": false,
  "github-actions.workflows.pinned.workflows": [],
  "i18n-ally.annotations": true,
  "i18n-ally.displayLanguage": "en",
  "i18n-ally.enabledFrameworks": ["vue"],
  "i18n-ally.extract.ignored": [
    "LivroLog",
    "preferred",
    "loading-amazon-links"
  ],
  "i18n-ally.keysInUse": [
    "admin.activity.book-added",
    "admin.book-updated",
    "loading-amazon-links",
    "preferred",
    "admin.activity.book-read",
    "admin.activity.book-started",
    "admin.activity.days-ago",
    "admin.activity.hours-ago",
    "admin.activity.just-now",
    "admin.activity.minutes-ago",
    "admin.activity.review-written",
    "admin.activity.user-followed",
    "amazon-group-americas",
    "amazon-group-asia-pacific",
    "amazon-group-europe",
    "amazon-group-middle-east-africa",
    "amazon-store-ae",
    "amazon-store-au",
    "amazon-store-be",
    "amazon-store-br",
    "amazon-store-ca",
    "amazon-store-de",
    "amazon-store-eg",
    "amazon-store-es",
    "amazon-store-fr",
    "amazon-store-ie",
    "amazon-store-in",
    "amazon-store-it",
    "amazon-store-jp",
    "amazon-store-mx",
    "amazon-store-nl",
    "amazon-store-pl",
    "amazon-store-sa",
    "amazon-store-se",
    "amazon-store-sg",
    "amazon-store-tr",
    "amazon-store-uk",
    "amazon-store-us",
    "amazon-store-za",
    "language-de",
    "language-en",
    "language-es",
    "language-fr",
    "language-it",
    "language-ja",
    "language-pt",
    "language-pt_br",
    "language-pt_pt",
    "language-ru",
    "language-zh",
    "months.apr",
    "months.aug",
    "months.dec",
    "months.feb",
    "months.jan",
    "months.jul",
    "months.jun",
    "months.mar",
    "months.may",
    "months.nov",
    "months.oct",
    "months.sep",
    "reading-statuses.abandoned",
    "reading-statuses.on_hold",
    "reading-statuses.re_reading",
    "reading-statuses.read",
    "reading-statuses.reading",
    "reading-statuses.want_to_read"
  ],
  "i18n-ally.keystyle": "nested",
  "i18n-ally.localesPaths": ["webapp/src/locales"],
  "i18n-ally.namespace": false,
  "i18n-ally.pathMatcher": "{locale}.json",
  "i18n-ally.sortKeys": true,
  "i18n-ally.sourceLanguage": "en",
  "intelephense.diagnostics.undefinedMethods": false,
  "intelephense.diagnostics.undefinedTypes": false,
  "php.validate.enable": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "yaml.completion": false,
  "yaml.format.enable": false,
  "yaml.hover": false,
  "yaml.schemaStore.enable": false,
  "yaml.validate": false
}
